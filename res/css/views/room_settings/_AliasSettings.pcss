.mx_AliasSettings_localAddresses {
    cursor: pointer;
    color: var(--cpd-color-text-primary);
    font-weight: var(--cpd-font-weight-semibold);
    text-decoration: underline;
    list-style: none;
    width: fit-content;

    /* list-style doesn't do it for webkit */
    &::-webkit-details-marker {
        display: none;
    }
}
